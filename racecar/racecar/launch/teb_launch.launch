<launch>
 <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">

  <rosparam file="$(find racecar)/launch/teb_params/costmap_common_params.yaml" command="load" ns="global_costmap" />
  <rosparam file="$(find racecar)/launch/teb_params/costmap_common_params.yaml" command="load" ns="local_costmap" />

  <rosparam file="$(find racecar)/launch/teb_params/global_costmap_params.yaml" command="load" />
  <rosparam file="$(find racecar)/launch/teb_params/local_costmap_params.yaml" command="load" />
  <rosparam file="$(find racecar)/launch/teb_params/teb_local_planner_params.yaml" command="load" />

  <param name="base_global_planner" type="string" value="navfn/NavfnROS" />
  <param name="base_local_planner" value="teb_local_planner/TebLocalPlannerROS" />

  <param name="controller_frequency" value="10.0" />
 
  <remap from="odom" to="/vesc/odom" />
 </node>

</launch>
